#include<bits/stdc++.h>
using namespace std;

int main(){
   vector<int>a = {5,7,8,9,11};
   vector<int>b = {2,3,4,10,13,25};
   
   int n = a.size();
   int m = b.size();
   
   int i = 0;
   int j = 0;
   
   while((i < n) && (j < m)){
       if(a[i] <= b[j]){
           cout << a[i] << " ";
           i++;
       }
       else{
           cout << b[j] << " ";
           j++;
       }
   }
   
   while(i < n){
       cout << a[i] << " ";
       i++;
   }
   while(j < m){
       cout << b[j] << " ";
       j++;
   }
    
}
