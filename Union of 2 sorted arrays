class Solution {
  public:
    // a,b : the arrays
    // Function to return a list containing the union of the two arrays.
    vector<int> findUnion(vector<int> &a, vector<int> &b) {
        // Your code here
        set<int>s1;
        vector<int>ans;
        
        for(int i = 0; i < a.size(); i++){
            s1.insert(a[i]);
        }
        for(int i = 0; i < b.size(); i++){
            s1.insert(b[i]);
        }
        for(auto it : s1){
            ans.push_back(it);
        }
        
        
        return ans;
        // return vector with correct order of elements
    }
};
