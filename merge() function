#include<bits/stdc++.h>
using namespace std;

void merge(int a[], int low, int mid, int high){
    int n = (mid - low + 1);
    int m = (high - mid);
    int left[n];
    int right[m];
    
    for(int i = 0; i < n; i++){
        left[i] = a[low + i];
    }
    for(int i = 0; i < m; i++){
        right[i] = a[mid + i + 1];
    }
    
    int i = 0; int j = 0; int k = low;
    
    while((i < n) && (j < m)){
        if(left[i] <= right[j]){
            a[k] = left[i];
            i++;
            k++;
        }
        else{
            a[k] = right[j];
            j++;
            k++;
        }
    }
    
    while(i < n){
        a[k] = left[i];
        i++;
        k++;
    }
    while(j < m){
        a[k] = right[j];
        j++;
        k++;
    }
        
}

int main(){
    int arr[6] = {3,4,5,6,2,3};
    
    
    merge(arr, 0 , 3 , 5);
    
    for(int i = 0; i < 6 ; i++){
        cout << arr[i] << " ";
    }
    
}
