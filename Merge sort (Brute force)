#include<bits/stdc++.h>
using namespace std;

int main(){
   vector<int>a = {5,7,8,9,11};
   vector<int>b = {2,3,4,10,13,25};
   
   int n = a.size();
   int m = b.size();
   
   vector<int>ans;
   
   for(int i = 0; i < n; i++){
       ans.push_back(a[i]);
   }
   for(int i = 0; i < m; i++){
       ans.push_back(b[i]);
   }
   
   cout << "Original array : " << endl;
   for(int i = 0; i < ans.size(); i++){
       cout  << ans[i] << " " ;
   }
   cout << endl;
   
   cout << "Sorted using sort() function : "<<endl;
   
   sort(ans.begin() , ans.end());
   
   for(int i = 0; i < ans.size(); i++){
       cout  << ans[i] << " " ;
   }
    
    
    
}
