#include<bits/stdc++.h>
using namespace std;

void print(vector<vector<int>>&arr ,int rows ,int cols){
    for(int i = 0; i < rows; i++){
        for(int j = 0; j < cols; j++){
            cout << arr[i][j] << " ";
        }
        cout << endl;
    }
}
int main(){
    int rows,cols;
    cin >> rows >> cols;
    vector<vector<int>>arr;
    
    for(int i = 0 ; i < rows; i++){
        vector<int>v;
        for(int j = 0; j < cols; j++){
            v.push_back(i + j + 1);
            arr.push_back(v);
        }
    }
    
    print(arr, rows , cols);
}
