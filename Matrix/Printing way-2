#include<bits/stdc++.h>
using namespace std;

void print(vector<vector<int>>&arr ,int rows,int cols){
    for(int i = 0; i < rows; i++){
        for(int j = 0; j < cols; j++){
            cout << arr[i][j] << " ";
        }
        cout << endl;
    }
}
int main(){
    int rows,cols;
    cin >> rows >> cols;
    vector<vector<int>>arr(rows , vector<int>(cols));
    
    for(int i = 0 ; i < rows; i++){
        for(int j = 0; j < cols; j++){
            arr[i][j] = i+j+1;
        }
    }
    
    print(arr , rows , cols);
}
