void rightDiagonal(vector<vector<int>>&arr , int rows, int cols){
    for(int i = 0; i < min(rows,cols); i++){
            cout << arr[i][cols - i - 1] << " ";
    }
}

void leftDiagonal(vector<vector<int>>&arr , int rows , int cols){
    for(int i = 0; i < min(rows,cols); i++){
        cout << arr[i][i] << " " ;
    }
}
