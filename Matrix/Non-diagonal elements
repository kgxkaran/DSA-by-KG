void nonDiagonal(vector<vector<int>>&arr,int rows,int cols){
    for(int i = 0; i < rows; i++){
        for(int j = 0; j < cols; j++){
            // Primary diagonal (i==j)
            // Secondary diagonal (j==cols-1-i)
            if(i == j || j == (cols-i-1)) continue;

            // Non-diagonal elements
            cout << arr[i][j] << " ";
        }
    }
}
